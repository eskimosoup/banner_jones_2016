<aside class="page-additional-information">
  <div class="service-page-additional-information-key-team-members">
    <div class="service-page-additional-information-key-team-members-title">
      <h3>
        Talk to our<br />
        <%= @service.title %> team
      </h3>
    </div>

    <div class="service-page-additional-information-key-team-members-profiles">
      <div class="multiple-team-members-slick">
        <%= render partial: 'services/shared/key_team_member_avatars', collection: @service.key_team_members, as: :service_team_member %>
      </div>
    </div>
  </div>

  <%= render 'pages/shared/call_to_actions' %>

  <% if @service.rich_snippet %>
    <div class="service-rich-snippets">
      <%= render(
        "rich_snippets/service_rich_snippet",
        rich_snippet: @service.rich_snippet
       ) %>
    </div>
  <% end %>
</aside>
