<div class="service-page-additional-information-key-team-members">
  <div class="service-page-additional-information-key-team-members-title">
    <h3>
      Talk to our<br />
      <%= @service.inherit_sidebar_text? ? @service.root.title : @service.title %> team
    </h3>
  </div>

  <div class="service-page-additional-information-key-team-members-profiles">
    <div class="multiple-team-members-slick">
      <%= render partial: 'services/shared/key_team_member_avatars', collection: @service.team_members.key_team_members, cached: true, as: :team_member %>
    </div>
  </div>
</div>

<%= render 'pages/shared/call_to_actions_solicitors' unless @service.layout == 'wealth_management' %>
<%= render 'pages/shared/call_to_actions_wealth_management' if @service.layout == 'wealth_management' %>

<% if @rich_snippet.present? %>
  <div class="service-rich-snippets">
    <%= render(
      "rich_snippets/rich_snippet",
      rich_snippet: @rich_snippet
    ) %>
  </div>
<% end %>
